<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.minsait.onesait.platform.android.healthcheckapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainHealthActivity"></activity>
        <activity android:name=".FormActivity" />
        <activity android:name=".HistActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".SpecialistActivity" />
        <activity android:name=".CitiInboxActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".AdviseActivity"></activity>
    </application>

</manifest>
