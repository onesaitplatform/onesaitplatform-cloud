{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DataModel WasteContainer Schema",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "refWasteContainerModel": {
            "type": "string"
        },
        "refWasteContainerIsle": {
            "type": "string"
        },
        "serialNumber": {
            "type": "string"
        },
        "location": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string"
                },
                "coordinates": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                }
            },
            "required": [
                "type",
                "coordinates"
            ]
        },
        "fillingLevel": {
            "type": "number"
        },
        "dateLastEmptying": {
            "type": "string"
        },
        "dateNextActuation": {
            "type": "string"
        },
        "status": {
            "type": "string"
        },
        "category": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "refDevice": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "id",
        "type",
        "refWasteContainerModel",
        "refWasteContainerIsle",
        "serialNumber",
        "location",
        "fillingLevel",
        "dateLastEmptying",
        "dateNextActuation",
        "status",
        "category",
        "refDevice"
    ]
}