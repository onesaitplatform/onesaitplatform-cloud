{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "DataModel Off Street Parking Schema",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "type": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "category": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "chargeType": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "requiredPermit": {
            "type": "null"
        },
        "layout": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "maximumParkingDuration": {
            "type": "string"
        },
        "location": {
            "type": "object",
            "properties": {
                "coordinates": {
                    "type": "array",
                    "items": {
                        "type": "number"
                    }
                },
                "type": {
                    "type": "string"
                }
            },
            "required": [
                "coordinates",
                "type"
            ]
        },
        "allowedVehicleType": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "totalSpotNumber": {
            "type": "integer"
        },
        "availableSpotNumber": {
            "type": "integer"
        },
        "address": {
            "type": "object",
            "properties": {
                "streetAddress": {
                    "type": "string"
                },
                "addressLocality": {
                    "type": "string"
                },
                "addressCountry": {
                    "type": "string"
                }
            },
            "required": [
                "streetAddress",
                "addressLocality",
                "addressCountry"
            ]
        },
        "description": {
            "type": "string"
        },
        "dateModified": {
            "type": "string"
        }
    },
    "required": [
        "id",
        "type",
        "name",
        "category",
        "chargeType",
        "requiredPermit",
        "layout",
        "maximumParkingDuration",
        "location",
        "allowedVehicleType",
        "totalSpotNumber",
        "availableSpotNumber",
        "address",
        "description",
        "dateModified"
    ]
}